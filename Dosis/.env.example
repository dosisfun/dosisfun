# Dosis App Environment Variables
# Copy this file to .env and fill in your values

# Starknet Network Configuration
EXPO_PUBLIC_STARKNET_NETWORK=SN_SEPOLIA
EXPO_PUBLIC_STARKNET_RPC_URL=your_rpc

# NFT Contract Addresses
EXPO_PUBLIC_DOSIS_NFT_CONTRACT=nft_contract

# Voyager API Configuration
EXPO_PUBLIC_VOYAGER_BASE_URL=https://sepolia.voyager.online/api
EXPO_PUBLIC_VOYAGER_EXPLORER_URL=https://sepolia.voyager.online

# IPFS Gateway
EXPO_PUBLIC_IPFS_GATEWAY=https://ipfs.io/ipfs/

# Aegis Configuration
EXPO_PUBLIC_AEGIS_APP_NAME=dosisfun
EXPO_PUBLIC_AEGIS_APP_ID=your_app_id
